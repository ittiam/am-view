{
  "name": "am-view",
  "version": "0.1.2",
  "description": "Mobile UI based on Vue.js",
  "author": "czh <ittiamc@gmail.com>",
  "main": "lib/index.js",
  "style": "lib/style.css",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.dev.js --inline --hot --port 8200",
    "build": "cross-env NODE_ENV=production webpack --config build/webpack.prod.js --progress --hide-modules",
    "demo": "cross-env NODE_ENV=production webpack --config build/webpack.demo.js --progress --hide-modules",
    "deploy": "npm run demo && gh-pages -d dist"
  },
  "keywords": [
    "mobile",
    "ui",
    "vue.js",
    "less",
    "vue2",
    "webpack2"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ittiam/am-view.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ittiam/am-view/issues"
  },
  "homepage": "https://github.com/ittiam/am-view#readme",
  "dependencies": {
    "vue-swipe": "^2.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.2",
    "babel-core": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-stage-2": "^6.13.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "gh-pages": "^0.12.0",
    "html-webpack-plugin": "^2.24.1",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "postcss-loader": "^1.1.1",
    "progress-bar-webpack-plugin": "^1.9.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "vue": "^2.0.1",
    "vue-loader": "^9.7.0",
    "vue-router": "^2.0.1",
    "vue-style-loader": "^1.0.0",
    "webpack": "^2.1.0-beta.27",
    "webpack-dev-server": "^2.1.0-beta.11",
    "webpack-hot-middleware": "^2.13.2",
    "webpack-merge": "^0.15.0"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  }
}
